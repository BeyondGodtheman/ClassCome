<?xml version="1.0" encoding="utf-8"?>
<resources>

    <!--个人中心子条目-->
    <declare-styleable name="MineItemLayout">
        <attr name="mine_layout_icon" format="reference"/>
        <attr name="mine_layout_text_left" format="string"/>
        <attr name="mine_layout_text_right" format="string"/>
        <attr name="mine_layout_text_show_default" format="boolean"/>
    </declare-styleable>

    <!--下拉刷新-->
    <declare-styleable name="KRefreshLayout">
        <attr name="k_pin_content" format="boolean" />
        <attr name="k_keep_header" format="boolean" />
        <attr name="k_refresh_enable" format="boolean" />
        <attr name="k_duration_offset" format="integer" />
        <attr name="k_def_refresh_height" format="dimension" />
        <attr name="k_def_max_offset" format="dimension|enum">
            <enum name="match_parent" value="-1" />
        </attr>
    </declare-styleable>
</resources>